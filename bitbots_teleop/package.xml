<?xml version="1.0"?>
<package format="2">
  <name>bitbots_teleop</name>
  <version>1.0.0</version>
  <description>The bitbots_teleop takes messages from the joy node and converts them to messages for /cmd_vel.</description>

  <author email="5guelden@informatik.uni-hamburg.de">Jasper Güldenstein</author>
  <maintainer email="5guelden@informatik.uni-hamburg.de">Jasper Güldenstein</maintainer>
  <license>BSD</license>

  <buildtool_depend>catkin</buildtool_depend>

  <depend>rospy</depend>
  
  <exec_depend>humanoid_league_msgs</exec_depend>
  <exec_depend>std_msgs</exec_depend>


  <export>
    <bitbots_documentation>
      <language>python2</language>
      <status>tested_robot</status>
    </bitbots_documentation>
  </export>
</package>
