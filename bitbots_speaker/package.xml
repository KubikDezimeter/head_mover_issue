<?xml version="1.0"?>
<package>
  <name>bitbots_speaker</name>
  <version>1.0.0</version>
  <description>The bitbots_speaker package provides an easy way to do audio speech output via espeak.
  Messages send to the /speak topic are queued and spoken in another thread depending on their priority. </description>

  <author email="0bestman@informatik.uni-hamburg.de">Marc Bestmann</author>
  <maintainer email="info@bit-bots.de">Hamburg Bit-Bots</maintainer>
  <license>BSD</license>

  <buildtool_depend>catkin</buildtool_depend>

  <build_depend>std_msgs</build_depend>
  <build_depend>message_generation</build_depend>
  <build_depend>dynamic_reconfigure</build_depend>
  <build_depend>humanoid_league_msgs</build_depend>
  <run_depend>std_msgs</run_depend>

  <run_depend>message_runtime</run_depend>
  <run_depend>dynamic_reconfigure</run_depend>
  <run_depend>humanoid_league_msgs</run_depend>

  <export>
  </export>
</package>