<?xml version="1.0"?>
<package>
  <name>humanoid_league_team_communication</name>
  <version>1.0.0</version>
  <description>The humanoid_league_team_communication package provides a ROS wrapper for the MiteCom standard communication
  protocol developed by the FUmanoids. </description>

  <author email="0bestman@informatik.uni-hamburg.de">Marc Bestmann</author>
  <author email="5flemmin@informatik.uni-hamburg.de">Tanja Flemming</author>
  <maintainer email="info@bit-bots.de">Hamburg Bit-Bots</maintainer>
  <license>BSD</license>

  <buildtool_depend>catkin</buildtool_depend>

  <build_depend>std_msgs</build_depend>
  <build_depend>humanoid_league_msgs</build_depend>
  <build_depend>roscpp</build_depend>
  <build_depend>geometry_msgs</build_depend>

  <run_depend>std_msgs</run_depend>
  <run_depend>humanoid_league_msgs</run_depend>
  <run_depend>roscpp</run_depend>
  <run_depend>geometry_msgs</run_depend>

  <export>
    <bitbots_documentation>
        <status>tested_robot</status>
      <language>c++</language>
    </bitbots_documentation>
  </export>
</package>
